{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "target": "ES2020",
    "module": "ES2020",
    "lib": ["ES2020", "DOM", "DOM.Iterable"],
    "outDir": "../dist/Client",
    "rootDir": "./src",
    "types": [],
    "moduleResolution": "node",
    "allowJs": true,
    "checkJs": false,
    "jsx": "preserve",
    "resolveJsonModule": true
  },
  "include": [
    "./**/*"
  ],
  "exclude": [
    "node_modules",
    "../dist"
  ]
} 